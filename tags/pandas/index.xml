    .agg(
        F.count(&#39;n&#39;).alias(&#39;n_count&#39;),
        F.min(&#39;x&#39;).alias(&#39;x_min&#39;),
        F.max(&#39;x&#39;).alias(&#39;x_max&#39;),
        F.min(&#39;y&#39;).alias(&#39;y_min&#39;),
